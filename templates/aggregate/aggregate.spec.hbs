{{/ifEquals}}
{{#ifEquals type "number"}}
import { CustomNumberValueObject, DomainId } from "types-ddd";
{{/ifEquals}}
import {{pascalCase name}}Aggregate from "../{{dashCase name}}.aggregate";

describe('{{lowerCase name}}.aggregate', () => {
	it('should create a {{lowerCase name}} aggregate with success', () => {

		const ID = DomainId.create();
		{{#ifEquals type "string"}}
		const key = CustomStringValueObject.create("valid value").getResult();
		{{/ifEquals}}
		{{#ifEquals type "number"}}
		const key = CustomNumberValueObject.create(1000).getResult();
		{{/ifEquals}}

		const aggregate = {{pascalCase name}}Aggregate.create({ ID, key });
		expect(aggregate.isSuccess).toBeTruthy();
	});
});
